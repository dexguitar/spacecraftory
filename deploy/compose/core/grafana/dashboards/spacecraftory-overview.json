{
  "id": null,
  "title": "Spacecraftory Overview",
  "description": "Business metrics for Order and Assembly services",
  "tags": ["spacecraftory", "orders", "assembly"],
  "timezone": "browser",
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "annotations": {
    "list": []
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Orders Rate",
      "description": "Number of orders created per second",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "rate(orders_total[1m])",
          "legendFormat": "Orders/sec",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0,
          "color": {
            "mode": "fixed",
            "fixedColor": "green"
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 2,
      "title": "Total Orders",
      "description": "Cumulative number of orders created",
      "type": "stat",
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "orders_total",
          "legendFormat": "Total Orders",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 100},
              {"color": "red", "value": 1000}
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 3,
      "title": "Total Revenue",
      "description": "Cumulative revenue from all orders",
      "type": "stat",
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
      "targets": [
        {
          "expr": "orders_revenue_total",
          "legendFormat": "Revenue",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 10000},
              {"color": "red", "value": 100000}
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 4,
      "title": "Revenue Rate",
      "description": "Revenue per second",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "rate(orders_revenue_total[1m])",
          "legendFormat": "Revenue/sec",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "min": 0,
          "color": {
            "mode": "fixed",
            "fixedColor": "blue"
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 5,
      "title": "Assembly Duration p95",
      "description": "95th percentile of spacecraft assembly duration",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(assembly_duration_seconds_bucket[1m])) by (le))",
          "legendFormat": "Assembly p95",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0,
          "color": {
            "mode": "fixed",
            "fixedColor": "orange"
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 6,
      "title": "Assembly Duration Distribution",
      "description": "Histogram of assembly times",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(assembly_duration_seconds_bucket[1m])) by (le))",
          "legendFormat": "p50",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        },
        {
          "expr": "histogram_quantile(0.90, sum(rate(assembly_duration_seconds_bucket[1m])) by (le))",
          "legendFormat": "p90",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(assembly_duration_seconds_bucket[1m])) by (le))",
          "legendFormat": "p99",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 7,
      "title": "Assemblies Rate",
      "description": "Number of assemblies completed per second",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "targets": [
        {
          "expr": "rate(assembly_duration_seconds_count[1m])",
          "legendFormat": "Assemblies/sec",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0,
          "color": {
            "mode": "fixed",
            "fixedColor": "purple"
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    }
  ],
  "version": 1,
